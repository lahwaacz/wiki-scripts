# Environment variables for the mediawiki database container
MW_DB_TYPE=postgres
MW_DB_NAME=mediawiki
MW_DB_USER=mediawiki
MW_DB_PASSWORD=very-secret

# The protocol and server name to use in URLs
MW_SERVER=http://localhost:8080
# The relative path of the wiki in the web server
MW_SCRIPT_PATH=""
# The name of the wiki
MW_NAME=wiki-scripts-tests
# The username of the wiki administrator
MW_USER=wiki-scripts
# The password for the wiki administrator
MW_PASSWORD=super-secret-password
